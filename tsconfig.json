{
    "compilerOptions": {
        "target": "ES2021",
        "module": "CommonJS",
        "lib": [
            "es2021"
        ],
        "skipLibCheck": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "strict": true,
        "baseUrl": ".",
        "paths": {
            "@prisma/client": [
                "./backend/node_modules/@prisma/client"
            ],
            ".prisma/client": [
                "./backend/node_modules/.prisma/client"
            ]
        }
    }
}